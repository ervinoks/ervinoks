{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.UserName}}@{{.HostName}} : {{.PWD}}",
  "version": 4,
  "enable_cursor_positioning": true,
  "auto_upgrade": true,
  "shell_integration": true,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "template": "\uf120 {{ .Name }} ",
          "foreground": "#ffffff",
          "background": "#00a88b"
        },
        {
          "type": "os",
          "style": "accordion",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#0077c2",
          "background": "#fbfbfb",
          "powerline_symbol": "\ue0b0",
          "options": {
            "display_distro_name": false,
            "macos": "\uf179",
            "linux": "\uebc6",
            "windows": "\ue62a",
            "alma": "\uf31d",
            "almalinux": "\uf31d",
            "almalinux9": "\uf31d",
            "alpine": "\uf300",
            "android": "\ue70e",
            "aosc": "\uf301",
            "arch": "\uf303",
            "centos": "\uf304",
            "coreos": "\uf305",
            "debian": "\uf306",
            "deepin": "\uf321",
            "devuan": "\uf307",
            "elementary": "\uf309",
            "endeavouros": "\uf322",
            "fedora": "\uf30b",
            "freebsd": "\udb82\udce0",
            "gentoo": "\udb82\udce8",
            "kali": "\uf327",
            "mageia": "\uf310",
            "manjaro": "\uf312",
            "mint": "\udb82\udced",
            "neon": "\uf331",
            "nixos": "\uf313",
            "opensuse": "\uf314",
            "opensuse-tumbleweed": "\uf314",
            "raspbian": "\uf315",
            "redhat": "\udb84\udd1b",
            "rocky": "\uf32b",
            "sabayon": "\uf317",
            "slackware": "\uf318",
            "ubuntu": "\uf31b"
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "template": "{{ if .SSHSession }}\ueb3a {{ end }}{{ .UserName }}<#000000>@</><#e06c75>{{ .HostName }}</> ",
          "foreground": "#0077c2",
          "background": "#fbfbfb",
          "powerline_symbol": "\ue0b0",
          "options": {
            "display_host": true
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "template": " \ue799\u2800",
          "foreground": "#ffffff",
          "background": "#e06c75",
          "powerline_symbol": "\ue0b0"
        },
        {
          "type": "path",
          "style": "powerline",
          "template": " \uf07b {{ path .Path .Location }} ",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "powerline_symbol": "\ue0b0",
          "options": {
            "folder_separator_icon": "/",
            "home_icon": "~",
            "max_depth": 2,
            "style": "powerlevel"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "template": " {{ url .UpstreamIcon .UpstreamURL }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \udb82\udd0c {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#e6e6e6",
          "background": "#f34f29",
          "powerline_symbol": "\ue0b4",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "options": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "docker",
          "style": "powerline",
          "template": " \uf308 {{ .Context }} ",
          "foreground": "#ffffff",
          "background": "#0B59E7",
          "powerline_symbol": "\ue0b4",
          "options": {
            "display_mode": "files"
          }
        },
        {
          "type": "project",
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\udb80\udda7 {{.Version}}{{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }} ",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "powerline_symbol": "\ue0b4"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "upgrade",
          "style": "plain",
          "template": "\udb80\uddda ",
          "foreground": "#fc4949",
          "background": "transparent"
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#3C873A",
          "background": "#303030",
          "options": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          }
        },
        {
          "type": "react",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue7ba {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#20232a",
          "background": "#61DAFB"
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#FFE873",
          "background": "#306998",
          "options": {
            "display_mode": "context"
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#F8981D",
          "background": "#5382A1"
        },
        {
          "type": "cmake",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue794 cmake {{ .Full }} ",
          "foreground": "#E8EAEE",
          "background": "#1E9748"
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#0d6da8",
          "background": "#0e0e0e"
        },
        {
          "type": "haskell",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue777 {{ .Full }}",
          "foreground": "#906cff",
          "background": "#100e23",
          "options": {
            "stack_ghc_mode": "package"
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#06aad5",
          "background": "#ffffff"
        },
        {
          "type": "rust",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#925837",
          "background": "#f3f0ec"
        },
        {
          "type": "dart",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#055b9c",
          "background": "#e1e8e9"
        },
        {
          "type": "angular",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#ce092f",
          "background": "#ffffff"
        },
        {
          "type": "nx",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}",
          "foreground": "#ffffff",
          "background": "#1e293b"
        },
        {
          "type": "julia",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<#ca3c34>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#359a25",
          "background": "#945bb3"
        },
        {
          "type": "ruby",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#9c1006",
          "background": "#ffffff"
        },
        {
          "type": "azfunc",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\uf104<#f5bf45>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#5398c2",
          "background": "#ffffff"
        },
        {
          "type": "aws",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "foreground": "#faa029",
          "background": "#565656"
        },
        {
          "type": "kubectl",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "foreground": "#ffffff",
          "background": "#316ce4"
        },
        {
          "type": "copilot",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\uf4b8 {{.Premium.Percent.Gauge}} {{ .Premium.Used }}/{{ .Premium.Limit }}",
          "foreground": "#111111",
          "background": "#fee898",
          "cache": {
            "duration": "5m",
            "strategy": "session"
          },
          "options": {
            "http_timeout": 1000
          }
        },
        {
          "type": "wakatime",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "WakaTime {{ secondsRound .CumulativeTotal.Seconds }}",
          "foreground": "#ffffff",
          "background": "#303030",
          "cache": {
            "duration": "10m0s",
            "strategy": "folder"
          },
          "options": {
            "http_timeout": 1000,
            "url": "https://wakatime.com/api/v1/users/current/summaries?start=today&end=today&api_key={{ .Env.WAKATIME_API_KEY }}"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\u2502 ",
          "foreground": "#00a88b",
          "background": "transparent"
        },
        {
          "type": "spotify",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .Icon }} {{ if ne .Status \"stopped\" }}{{ .Artist }} \ue0bd {{ .Track }}{{ end }}",
          "foreground": "#011627",
          "background": "#1DB954",
          "options": {
            "playing_icon": "\uf1bc"
          }
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .Templates }}",
          "foreground": "#ffffff",
          "background": "#f36943",
          "templates_logic": "first_match",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#00c2ab{{end}}",
            "{{if eq \"Discharging\" .State.String}}#efa161{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "templates": [
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 5) }}\udb80\udc83 {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 10) }}\udb80\udc8e {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 20) }}\udb80\udc7a {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 30) }}\udb80\udc7b {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 40) }}\udb80\udc7c {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 50) }}\udb80\udc7d {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 60) }}\udb80\udc7e {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 70) }}\udb80\udc7f {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 80) }}\udb80\udc80 {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 90) }}\udb80\udc81 {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Discharging\" .State.String) (lt .Percentage 100) }}\udb80\udc82 {{ .Percentage }}%{{ end }}",
            "{{ if eq \"Discharging\" .State.String }}\udb80\udc79 {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 10) }}\udb82\udc9f {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 20) }}\udb82\udc9c {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 30) }}\udb80\udc86 {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 40) }}\udb80\udc87 {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 50) }}\udb80\udc88 {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 60) }}\udb82\udc9d {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 70) }}\udb80\udc89 {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 80) }}\udb82\udc9e {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 90) }}\udb80\udc8a {{ .Percentage }}%{{ end }}",
            "{{ if and (eq \"Charging\" .State.String) (lt .Percentage 100) }}\udb80\udc8b {{ .Percentage }}%{{ end }}",
            "{{ if eq \"Charging\" .State.String }}\udb80\udc84 {{ .Percentage }}%{{ end }}"
          ]
        },
        {
          "type": "connection",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ if eq .Type \"wifi\" }}\udb82\udd28 {{.SSID}}{{ else if eq .Type \"ethernet\" }}\udb80\ude00 {{.Type}}{{ else if eq .Type \"bluetooth\" }}\uf293 {{.Type}}{{ else if eq .Type \"cellular\"}}\udb81\udca2 {{.Type}}{{ end }}",
          "foreground": "#ffffff",
          "background": "#39577D"
        },
        {
          "type": "sysinfo",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": " ",
          "template": "<#9fa9d4>\ue266</> {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB <#2C4564>\ue0b6</><#ffffff,#2C4564>{{ round .PhysicalPercentUsed .Precision }}%</><#2C4564,transparent>\ue0b4</>",
          "foreground": "#ffffff",
          "background": "#39577D"
        },
        {
          "type": "owm",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": "{{ .Weather }} {{ .Temperature }}{{ .UnitIcon }}",
          "foreground": "#ffffff",
          "background": "#00859a",
          "background_templates": [
            "{{ if (gt .Temperature 26) }}#ff6564{{ else if (gt .Temperature 18) }}#ffa040{{ else if (gt .Temperature 10) }}#159455{{ else }}#48a3ff{{ end }}"
          ],
          "templates": [
            "{{ if eq \"\ue32b\" .Weather }}üåô{{end}}",
            "{{ if eq \"\ue30d\" .Weather }}‚òÄÔ∏è{{end}}",
            "{{ if eq \"\ue37e\" .Weather }}\udb83\udf31{{end}}",
            "{{ if eq \"\ue302\" .Weather }}‚õÖ{{end}}",
            "{{ if eq \"\ue33d\" .Weather }}‚òÅÔ∏è{{end}}",
            "{{ if eq \"\ue312\" .Weather }}\ue312{{end}}",
            "{{ if eq \"\ue319\" .Weather }}üåßÔ∏è{{end}}",
            "{{ if eq \"\ue325\" .Weather }}\ue325 {{end}}",
            "{{ if eq \"\ue308\" .Weather }}\ue308 {{end}}",
            "{{ if eq \"\ue32a\" .Weather }}\ue32a {{end}}",
            "{{ if eq \"\ue30f\" .Weather }}\ue30f{{end}}",
            "{{ if eq \"\ue31a\" .Weather }}üå®Ô∏è{{end}}",
            "{{ if eq \"\ue313\" .Weather }}\udb81\udd91{{end}}",
            " {{ .Temperature }}{{ .UnitIcon }} "
          ],
          "cache": {
            "duration": "30m0s",
            "strategy": "session"
          },
          "options": {
            "api_key": "{{ .Env.OWM_API_KEY }}",
            "http_timeout": 2000,
            "location": "London,GB",
            "units": "metric"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\udb80\udced {{ .CurrentDate | date .Format }} \udb80\udd50",
          "foreground": "#ffffff",
          "background": "#6168ef",
          "options": {
            "time_format": "Monday Jan 02 \ue0bd 15:04"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "<#00a88b>\u2570\u2500</> {{if .Root}}#{{else}}${{end}}",
          "foreground": "#ffd54f",
          "background": "transparent"
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "status",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": "{{ if gt .Code 0 }}<#fa8f8b>\uf057</>{{ else }}<#ccffbe>\uf058</>{{ end }} ",
          "foreground": "#ffffff",
          "background": "#303030",
          "options": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .FormattedMs }}",
          "foreground": "#ffffff",
          "background": "#303030",
          "options": {
            "always_enabled": true,
            "style": "austin"
          }
        }
      ]
    }
  ],
  "valid_line": {
    "background": "transparent",
    "foreground": "#6febc8",
    "template": "\uf058 "
  },
  "error_line": {
    "background": "transparent",
    "foreground": "#eb6f92",
    "template": "\uf057 "
  },
  "transient_prompt": {
    "background": "#00a88b",
    "foreground": "#ffffff",
    "template": "<#00a88b,transparent>\ue0b6</>\uf120 {{ .Shell }}<#00a88b,transparent>\ue0b4</><#ffd54f,transparent> {{if .Root}}#{{else}}${{end}} </>"
  }
}

